
@{
    ViewBag.Title = "Prijavljeni";
    var lista = ViewBag.spisak;
}

<head>
    <style>
        .div1 {
            border: 1px solid cornflowerblue;
            padding: 10px 10px 10px 10px
        }
        #div1 {
            background-color: darkgray;
            height: 58px;
            width: 100%;
        }
        #but {
            width: 100px;
            font-size: 16px;
            text-align: center;
            color: white;
            padding: 12px 12px 12px 9px;
            margin: 5px 5px 5px 5px;
            border: 1px solid white;
            background-color: cornflowerblue;
            cursor: pointer;
        }
    </style>
</head>

<div id="div1">
    <form style="margin-right:10px" action="~/Home/Index"><input id="but" type="submit" value="HOME" /></form>
</div>

<h2 style="text-align:center">Korisnici koji su prijavljeni na trening</h2>
<br />
<br />
<hr>
<br />
<br />

<div class="div1">
    @foreach (var item in lista)
    {
        <p style="font-size:20px; font-family:sans-serif">Korisnik: @item</p><br />
    }
</div>

